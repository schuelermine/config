{
    "editor.fontFamily": "'Cascadia Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "explorer.sortOrder": "type",
    "editor.fontSize": 16,
    "debug.console.fontSize": 16,
    "markdown.preview.fontSize": 16,
    "editor.acceptSuggestionOnEnter": "off",
    "latex-workshop.view.pdf.viewer": "tab",
    "gitlens.defaultDateFormat": "dddd, DD. MMMM YYYY",
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "editor.largeFileOptimizations": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "telemetry.enableTelemetry": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.profiles.linux": {
        "dash": { "path": "dash" },
        "bash": { "path": "bash" },
        "zsh": { "path": "zsh" },
        "fish": { "path": "fish" },
        "tmux": { "path": "tmux" },
        "pwsh": { "path": "pwsh" },
        "ghci": { "path": "ghci" },
        "jshell": { "path": "jshell" },
        "deno": { "path": "deno" },
        "node": { "path": "node" }
    },
    "sonarlint.rules": {
        "java:S106": { "level": "off" },
        "Web:BoldAndItalicTagsCheck": { "level": "off" }
    },
    "terminal.integrated.fontSize": 16,
    "powershell.integratedConsole.showOnStartup": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "javascript.format.semicolons": "insert",
    "typescript.format.semicolons": "insert",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
    "security.workspace.trust.emptyWindow": true,
    "security.workspace.trust.untrustedFiles": "newWindow",
    "csharp.showOmnisharpLogOnError": false,
    "editor.codeLensFontFamily": "'Cascadia Code'",
    "editor.comments.ignoreEmptyLines": false,
    "editor.inlayHints.fontFamily": "'Cascadia Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.linkedEditing": true,
    "editor.maxTokenizationLineLength": 40000,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.showFoldingControls": "always",
    "editor.tabCompletion": "on",
    "editor.unfoldOnClickAfterEndOfLine": true,
    "editor.wrappingStrategy": "advanced",
    "editor.cursorBlinking": "solid",
    "editor.cursorSurroundingLines": 2,
    "editor.find.autoFindInSelection": "multiline",
    "diffEditor.codeLens": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.quickSuggestionsDelay": 0,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "files.autoGuessEncoding": true,
    "files.hotExit": "onExitAndWindowClose",
    "files.insertFinalNewline": true,
    "files.simpleDialog.enable": true,
    "workbench.experimental.useCustomHover": true,
    "workbench.list.horizontalScrolling": true,
    "workbench.list.keyboardNavigation": "filter",
    "workbench.list.multiSelectModifier": "alt",
    // "workbench.list.openMode": "doubleClick",
    "workbench.startupEditor": "readme",
    "workbench.sideBar.location": "right",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.settings.openDefaultKeybindings": true,
    "screencastMode.mouseIndicatorSize": 35,
    "window.dialogStyle": "custom",
    "window.menuBarVisibility": "compact",
    "window.titleBarStyle": "custom",
    "window.title": "${activeEditorShort}${separator}${rootName}${separator}${remoteName}${separator}VS Code",
    "window.titleSeparator": " Â· ",
    "js/ts.implicitProjectConfig.checkJs": true,
    "explorer.incrementalNaming": "smart",
    "explorer.sortOrderLexicographicOptions": "unicode",
    "search.collapseResults": "auto",
    "search.quickOpen.includeSymbols": true,
    "search.searchEditor.defaultNumberOfContextLines": 2,
    "search.showLineNumbers": true,
    "debug.console.fontFamily": "'Cascadia Code'",
    "workbench.iconTheme": "vs-nomo-dark",
    "workbench.colorTheme": "Default High Contrast",
    "editor.cursorStyle": "block-outline",
    "terminal.integrated.cursorStyle": "underline",
    "githubIssues.queries": [
        {
            "label": "Relevant Issues",
            "query": "default"
        },
        {
            "label": "Issues by me",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "githubPullRequests.queries": [
        {
            "label": "Review Needed",
            "query": "is:open review-requested:${user}"
        },
        {
            "label": "Assigned To Me",
            "query": "is:open assignee:${user}"
        },
        {
            "label": "Open PRs By Me",
            "query": "is:open author:${user}"
        },
        {
            "label": "PRs Here By Me",
            "query": "author:${user} repo:${owner}/${repository}"
        }
    ]
}
